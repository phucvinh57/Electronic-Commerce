    "@tsed/exceptions": "^7.3.0",
    "@tsed/json-mapper": "^7.3.0",
    "@tsed/logger": "^6.3.2",
    "@tsed/logger-file": "^6.3.2",
    "@tsed/openspec": "^7.3.0",
    "@tsed/platform-cache": "^7.3.0",
    "@tsed/platform-exceptions": "^7.3.0",
    "@tsed/platform-express": "^7.3.0",
    "@tsed/platform-log-middleware": "^7.3.0",
    "@tsed/platform-middlewares": "^7.3.0",
    "@tsed/platform-params": "^7.3.0",
    "@tsed/platform-response-filter": "^7.3.0",
    "@tsed/platform-views": "^7.3.0",
    "@tsed/prisma": "^7.3.0",
    "@tsed/schema": "^7.3.0",
    "@tsed/swagger": "^7.3.0",
    "ajv": "^8.11.0",
    "barrelsby": "^2.5.1",
    "body-parser": "^1.20.1",
