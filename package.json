    "@tsed/cli-plugin-eslint": "3.26.3",
    "@tsed/cli-plugin-jest": "3.26.3",
    "@tsed/cli-plugin-prisma": "3.26.3",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.2",
    "@types/method-override": "^0.0.32",
